local urlScript = 'https://raw.githubusercontent.com/gustavolunas/tibiaScript/refs/heads/master/Loaderv1'; 
do 
if not loadRemoteScript or type(loadRemoteScript) ~= 'function' then 
modules._G.HTTP.get(urlScript, function(script) 
(loadstring(script))(); 
end); 
end 
loadRemoteScript(urlScript); 
end
